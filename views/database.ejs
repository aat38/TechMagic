<!doctype html>

<html lang="en">
<head>
  <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.2.1/css/bootstrap.min.css" integrity="sha384-GJzZqFGwb1QTTN6wy59ffF1BuGJpLSa9DkKMp0DgiMDm4iYMj70gZWKYbI706tWS" crossorigin="anonymous">
</head>

<body>
<h1>Heroku enteries:</h1>
    <table class="table">
      <tbody><tr><th>Test Id</th><th>Name</th><th>Description</th></tr>
        <% JSON.stringify(data) %> 
        <% data.forEach(result=>{ %>
        <tr>
          <td><%= result.testid %></td>
           <td><%= result.name %></td>
          <td><%= result.description %></td>
        </tr>
        <% }); %>
      </tbody>
    </table>
</body>
</html>